<script>
    var element = layui.element,
        table = layui.table,
        laypage = layui.laypage,
        form = layui.form,
        upload = layui.upload,
        laydate = layui.laydate;

    layui.use('element', function () {
        var $ = layui.jquery, element = layui.element; //Tab的切换功能，切换事件监听等，需要依赖element模块
    });

    function SaveFormData() {
        debugger
        var basicData = $("#basic_form").serialize();
        var preData = $("#pre_form").serialize();
        var buildData = $("#build_form").serialize();
        var finishData = $("#finish_form").serialize();
        var importData = $("#import_form").serialize();

        Object.assign(basicData, preData);
        Object.assign(basicData, buildData);
        Object.assign(basicData, finishData);
        Object.assign(basicData, importData);

        var ajax = {
            url: '/ProjectPlanInfo/Create',
            type: 'post',
            async: false,
            data: basicData,
            dataType: "json",
            success: function (data) {
                if (data && data.Status == "1") {
                    window.parent.Reload();
                }
                else {
                    dialog.alert("保存失败！");
                }
            },
            error: function (XMLHttpRequest, textStatus, errorThrown) {
            }
        }

        $.ajax(ajax);
    }

    $(function () {
        $(".layui-input-date").each(function () {
            laydate.render({ elem: this, format: 'yyyy-MM-dd', type: 'date', trigger: 'click', theme: '#393D49', calendar: true });
        });
    });
</script>

